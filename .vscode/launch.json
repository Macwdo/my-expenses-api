    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Run all tests",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                "--keepdb",
            ],
        },
        {
            "name": "Run App Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/src/manage.py"
        },
    ]
}